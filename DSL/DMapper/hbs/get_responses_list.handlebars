{
"response": [
    {{#with data.[0].fields.filtered_responses.[0]}}
        {{#each this}}
        {
            "response": "{{@key}}",
            "text": {{#if this.[0].condition}}"{{{this.[1].text}}}"{{else}}"{{{this.[0].text}}}"{{/if}}
        }{{#unless @last}},{{/unless}}
        {{/each}}
    {{/with}}
]
}